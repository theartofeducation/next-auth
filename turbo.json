{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"]
    },
    "next-auth#build": {
      "dependsOn": ["^build"],
      "outputs": [
        "lib/**",
        "css/**",
        "jwt/**",
        "react/**",
        "next/**",
        "client/**",
        "providers/**",
        "core/**",
        "index.d.ts",
        "index.js",
        "adapters.d.ts",
        "middleware.d.ts",
        "middleware.js"
      ]
    },
    "test": {
      "dependsOn": ["lint", "build"],
      "outputs": []
    },
    "@next-auth/upstash-redis-adapter#test": {
      "dependsOn": [
        "lint",
        "build",
        "$UPSTASH_REDIS_KEY",
        "$UPSTASH_REDIS_URL"
      ]
    },
    "lint": {},
    "setup": {},
    "dev": {
      "cache": false
    },
    "next-auth-app#dev": {
      "cache": false,
      "dependsOn": ["@aoeu/next-auth#setup"]
    }
  }
}
